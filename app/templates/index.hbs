<div class="row">
  <div class="col-md-8 col-md-offset-2">
    <div class="well question-well">
      {{display-board}}
      <h3 class="text-center">Add your question here!</h3>
      {{new-question saveQuestion="saveQuestion"}}
    </div>

    <h3 class="text-center">Current Questions</h3>
    {{#each model.questions as |question|}}
      {{question-tile question=question}}
    {{/each}}
  </div>
</div>
